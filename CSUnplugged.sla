            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="12" CH="3. 它發覺對方很想要一塊積木來建個重要的東西。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="12" CH="4. 接著，它察覺到積木的數量很重要。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="12" CH="5. 它試著用青蛙這個玩笑去軟化對方。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="12" CH="6. 它表示已經準備好要討價還價了"/>
            <para LINESPMode="1"/>
            <para/>
            <ITEXT CH="當然，這種程度的對話藝術已經遠遠超過現在電腦的能力了！"/>
            <trail/>
            <PageItemAttributes/>
        </PAGEOBJECT>
        <PAGEOBJECT OwnPage="259" PTYPE="4" XPOS="420" YPOS="115176.5" WIDTH="223.5" HEIGHT="156" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="1" COLGAP="0" NAMEDLST="水平線" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LINESPMode="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" IFONT="Source Han Sans Normal" ISIZE="11" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="2" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="1" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 223.5 0 223.5 0 223.5 0 223.5 0 223.5 156 223.5 156 223.5 156 223.5 156 0 156 0 156 0 156 0 156 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 223.5 0 223.5 0 223.5 0 223.5 0 223.5 156 223.5 156 223.5 156 223.5 156 0 156 0 156 0 156 0 156 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="0" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="從前，有一個名叫杜魯門的北極燕鷗。杜魯門無家可歸。杜魯門需要一個窩。他飛到岸上。杜魯門在找一些樹枝。杜魯門沒有發現樹枝。他飛到苔原。他遇到了一個名為賀拉斯的北極熊。杜魯門問賀拉斯哪裡有一些樹枝。賀拉斯隱瞞了樹枝。賀拉斯告訴杜魯門說在冰山上有一些樹枝。杜魯門飛到冰山。他在找一些樹枝。他沒有發現樹枝。賀拉斯在找一些肉。他發現一些肉。他吃杜魯門。杜魯門死了。"/>
            <trail LINESPMode="1"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
