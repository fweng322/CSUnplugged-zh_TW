            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="唉，男人們都一樣。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：妳是指哪方面？"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="男人們總是不斷用許多問題來困擾我們。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：可以給我一個例子嗎？"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="噢，就像我男朋友要我來這裡。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：喔，妳男朋友要妳來這裡。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="他說我大部份時間都太憂鬱了。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：噢，聽到妳這麼憂鬱我感到很遺憾。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="那是真的，我很不快樂。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：那妳認為到這裡來能讓妳快樂一點嗎？"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="可以肯定的是，我需要幫忙。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：跟我談談妳的家人吧。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="..."/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：講到妳父親妳會聯想到什麼？"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="人："/>
            <tab FONT="Source Han Sans Normal" FONTSIZE="11"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="家暴。"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="電腦：這是為什麼妳男朋友要妳來這裡的原因嗎？"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Source Han Sans Normal" FONTSIZE="11" CH="...."/>
